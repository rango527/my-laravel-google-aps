{
  "name": "rango527/laravel-google-ads",
  "type": "library",
  "description": "Google Ads API for Laravel",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "laravel",
    "ads",
    "adwords",
    "google"
  ],
  "homepage": "https://github.com/rango527/laravel-google-ads",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikolaj Petersen",
      "email": "web.mobile497373@gmail.com",
      "homepage": "http://github.com/rango527"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "ext-soap": "*",
    "illuminate/console": "^5.1",
    "illuminate/support": "^5.1",
    "googleads/googleads-php-lib": "^41.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelGoogleAds\\": "src/LaravelGoogleAds"
    },
    "files": [
      "src/LaravelGoogleAds/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelGoogleAdsTests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelGoogleAds\\LaravelGoogleAdsProvider"
      ]
    }
  }
}
